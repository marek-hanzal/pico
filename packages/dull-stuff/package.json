{
	"version": "2.0.2",
	"name": "@use-pico/dull-stuff",
	"description": "Literally dull stuff. It contains some factory functions for the all-time-repeated stuff.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/use-pico/pico"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"sideEffects": false,
	"type": "module",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"files": [
		"src"
	],
	"scripts": {
		"build": "concurrently npm:build:*",
		"build:esbuild": "node esbuild.mjs",
		"build:types": "tsc --outDir lib",
		"lint": "eslint src",
		"cleanup": "concurrently npm:cleanup:*",
		"cleanup:sdk": "del-cli src/sdk/**",
		"cleanup:lib": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
	},
	"dependencies": {},
	"peerDependencies": {
		"@use-pico/form": "^2.0.10",
		"@use-pico/query": "^2.0.11",
		"@use-pico/repository": "^2.0.6",
		"@use-pico/rpc": "^2.0.6",
		"@use-pico/source-ui": "^2.0.6",
		"@use-pico/source": "^2.0.8",
		"@use-pico/ui-extra": "^2.0.10"
	},
	"devDependencies": {
		"@use-pico/esbuild": "^2.0.5",
		"@use-pico/eslint-config-eslint": "^2.0.4",
		"@use-pico/tsconfig": "^2.0.4",
		"@types/node": "^20.9.0",
		"concurrently": "^8.2.1",
		"del-cli": "^5.0.0",
		"eslint": "^8.45.0",
		"typescript": "^5.2.2"
	},
	"bugs": {
		"url": "https://github.com/use-pico/pico/issues"
	},
	"homepage": "https://github.com/use-pico/pico#readme"
}
