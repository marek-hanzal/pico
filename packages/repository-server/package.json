{
	"version": "1.0.1",
	"name": "@use-pico/repository-server",
	"description": "Server-side repository stuff.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/use-pico/pico"
	},
	"sideEffects": false,
	"type": "module",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"files": [
		"src"
	],
	"scripts": {
		"build": "concurrently npm:build:*",
		"build:esbuild": "node esbuild.mjs",
		"build:types": "tsc --outDir lib",
		"lint": "eslint src",
		"cleanup": "concurrently npm:cleanup:*",
		"cleanup:sdk": "del-cli src/sdk/**",
		"cleanup:lib": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
	},
	"dependencies": {
		"@use-pico/query": "^1.0.13",
		"@use-pico/repository": "^1.0.11",
		"@use-pico/rpc-server": "^1.0.1",
		"@use-pico/schema": "^1.0.19"
	},
	"devDependencies": {
		"@use-pico/esbuild": "^1.0.8",
		"@use-pico/eslint-config-eslint": "^1.0.10",
		"@use-pico/tsconfig": "^1.0.10",
		"@types/node": "^20.4.2",
		"@types/react": "^18.2.21",
		"concurrently": "^8.2.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.47.0",
		"typescript": "^5.2.2"
	},
	"bugs": {
		"url": "https://github.com/use-pico/pico/issues"
	},
	"homepage": "https://github.com/use-pico/pico#readme"
}
