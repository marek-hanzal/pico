{
	"version": "2.0.4",
	"name": "@use-pico/orm",
	"description": "ORM helper stuff, based on drizzle-orm.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/use-pico/pico"
	},
	"sideEffects": false,
	"type": "module",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"files": [
		"src"
	],
	"scripts": {
		"build": "concurrently npm:build:*",
		"build:esbuild": "node esbuild.mjs",
		"build:types": "tsc --outDir lib",
		"lint": "eslint src",
		"cleanup": "concurrently npm:cleanup:*",
		"cleanup:sdk": "del-cli src/sdk/**",
		"cleanup:lib": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
	},
	"dependencies": {
		"pg": "^8.11.3"
	},
	"peerDependencies": {
		"@use-pico/container": "^2.0.5",
		"kysely": "^0.26.3"
	},
	"devDependencies": {
		"@types/node": "^20.4.2",
		"@types/pg": "^8.10.9",
		"@types/react": "^18.2.15",
		"@use-pico/esbuild": "^2.0.4",
		"@use-pico/eslint-config-eslint": "^2.0.4",
		"@use-pico/tsconfig": "^2.0.4",
		"concurrently": "^8.2.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.45.0",
		"typescript": "^5.2.2"
	},
	"bugs": {
		"url": "https://github.com/use-pico/pico/issues"
	},
	"homepage": "https://github.com/use-pico/pico#readme"
}
