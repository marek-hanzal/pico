		"flat": "^6.0.1",
		"js-sha256": "^0.10.1",
		"luxon": "^3.4.1",
		"preferred-locale": "^2.0.1",
		"pupa": "^3.1.0"
	},
	"peerDependencies": {
		"@use-pico/navigation": "^2.0.4",
		"@use-pico/query": "^2.0.8",
		"@use-pico/schema": "^2.0.5",
		"@use-pico/store": "^2.0.4",
		"@use-pico/utils": "^2.0.6",
		"next": "^14.0.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@use-pico/esbuild": "^2.0.4",
