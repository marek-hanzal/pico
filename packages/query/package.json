	},
	"devDependencies": {
		"@use-pico/esbuild": "^1.0.5",
		"@use-pico/eslint-config-eslint": "^1.0.7",
		"@use-pico/tsconfig": "^1.0.7",
		"@types/node": "^20.4.2",
		"@types/react": "^18.2.20",
		"concurrently": "^8.2.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.45.0",
		"typescript": "^5.2.2"
