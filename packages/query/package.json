{
	"version": "1.0.10",
	"name": "@use-pico/query",
	"description": "This package only defines shape of the Query (how to get Entities or whatever you need)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/leight-core/viv.git"
	},
	"sideEffects": false,
	"type": "module",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"files": [
		"src"
	],
	"scripts": {
		"build": "concurrently npm:build:*",
		"build:esbuild": "node esbuild.mjs",
