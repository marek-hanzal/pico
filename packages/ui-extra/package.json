{
	"version": "1.0.12",
	"name": "@use-pico/ui-extra",
	"description": "Components with quite heavy dependencies on the other @pico packages.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/leight-core/viv.git"
	},
	"sideEffects": false,
	"type": "module",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"files": [
		"src"
	],
	"scripts": {
		"build": "concurrently npm:build:*",
		"build:esbuild": "node esbuild.mjs",
		"build:types": "tsc --outDir lib",
		"lint": "eslint src",
		"cleanup": "concurrently npm:cleanup:*",
		"cleanup:sdk": "del-cli src/sdk/**",
		"cleanup:lib": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
	},
	"dependencies": {
		"@mantine/core": "7.1.5",
		"@mantine/modals": "7.1.5",
		"@mantine/notifications": "7.1.5",
		"@use-pico/file": "^1.0.9",
		"@use-pico/hook": "^1.0.8",
		"@use-pico/i18n": "^1.0.8",
		"@use-pico/navigation": "^1.0.8",
		"@use-pico/query": "^1.0.10",
		"@use-pico/rpc": "^1.0.9",
		"@use-pico/schema": "^1.0.16",
		"@use-pico/ui": "^1.0.11",
		"@use-pico/utils": "^1.0.21",
		"@tabler/icons-react": "^2.39.0",
		"axios": "^1.5.1",
		"js-file-downloader": "^1.1.25",
		"next": "^13.5.4",
		"next-auth": "^4.22.1",
		"next-intl": "^2.20.2",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@use-pico/esbuild": "^1.0.5",
		"@use-pico/eslint-config-eslint": "^1.0.7",
		"@use-pico/tsconfig": "^1.0.7",
		"@types/node": "^20.4.2",
		"@types/react": "^18.2.15",
		"@types/uuid": "^9.0.2",
		"concurrently": "^8.2.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.45.0",
		"typescript": "^5.2.2"
	},
	"bugs": {
		"url": "https://github.com/leight-core/viv/issues"
	},
	"homepage": "https://github.com/leight-core/viv#readme"
}
