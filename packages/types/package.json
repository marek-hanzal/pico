{
	"version": "1.0.7",
	"name": "@use-pico/types",
	"description": "Various utility types (as from TypeScript and library-wise interfaces).",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/leight-core/viv.git"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"sideEffects": false,
	"type": "module",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"files": [
		"src"
	],
	"scripts": {
		"build": "concurrently npm:build:*",
		"build:esbuild": "node esbuild.mjs",
		"build:types": "tsc --outDir lib",
		"lint": "eslint src",
		"cleanup": "concurrently npm:cleanup:*",
		"cleanup:sdk": "del-cli src/sdk/**",
		"cleanup:lib": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
	},
	"dependencies": {
		"@total-typescript/ts-reset": "^0.5.1",
		"@use-pico/schema": "^1.0.16"
	},
	"devDependencies": {
		"@use-pico/esbuild": "^1.0.5",
		"@use-pico/eslint-config-eslint": "^1.0.7",
		"@use-pico/tsconfig": "^1.0.7",
		"@types/node": "^20.5.3",
		"@types/react": "^18.2.21",
		"concurrently": "^8.2.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.47.0",
		"typescript": "^5.2.2"
	},
	"bugs": {
		"url": "https://github.com/leight-core/viv/issues"
	},
	"homepage": "https://github.com/leight-core/viv#readme"
}
