{
	"$schema": "https://turborepo.org/schema.json",
	"globalDependencies": ["tsconfig.json"],
	"tasks": {
		"lint": {
			"dependsOn": ["^lint"],
			"outputs": [],
			"inputs": ["src/**"],
			"outputLogs": "new-only",
			"cache": false
		},
		"typecheck": {
			"dependsOn": ["^typecheck"],
			"outputs": [],
			"inputs": ["src/**"],
			"outputLogs": "new-only",
			"cache": false
		}
	}
}
